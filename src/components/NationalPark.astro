---
import { NationalPark, cardColor } from "../types/NationalPark";
import { cardDetail } from "../types/Park";
import Card from "./Card.astro";

interface Props {
  park: NationalPark;
}
const { park } = Astro.props;
---

<Card
  title={park.name}
  detail={cardDetail(park)}
  color={cardColor(park)}
  outline={park.visited === undefined}
/>
