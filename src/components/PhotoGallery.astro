---
import type { Photo } from "../types/Photo";
import PhotoPreview from "./PhotoPreview.astro";

export interface Props {
  photos: Photo[];
}
const { photos } = Astro.props as Props;
---

<div class="grid gap-3 grid-cols-1 md:grid-cols-2">
  {photos.map((p) => (
    <div>
      <PhotoPreview photo={p} />
    </div>
  ))}
</div>
